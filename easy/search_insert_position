#include<stdio.h>
int searchInsert(int* nums, int numsSize, int target) {
    for(int i=0;i<numsSize;i++){
        if(nums[i]>=target)
        return i;
    }
    if(nums[numsSize-1]<target)
    return numsSize;
    
    return 0;
}
int main()
{
    int nums[]={1,3,5,6};
    int target=5;
    int index=searchInsert(nums,4,target);
    printf("Index to insert %d is: %d\n",target,index);
    return 0;

}