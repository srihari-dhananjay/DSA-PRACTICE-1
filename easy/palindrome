#include<stdio.h>
bool isPalindrome(int x) {
long int y=x,digit=0;
while(y>0){
    int temp=y%10;
    digit=(digit*10)+temp;
    y/=10;
}
if(digit==x)
return 1;
else return 0;
}
int main(){
    int n;
    scanf("%d",&n);
    if(isPalindrome(n))
    printf("true");
    else
    printf("false");
    return 0;
}