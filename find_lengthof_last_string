#include <stdio.h>
int lengthOfLastWord(char* s) {
int a=strlen(s); 
int i=a-1;
int count=0;
while(i>=0&&s[i]==' '){
    i--;
}
while(i>=0&&s[i]!=' '){
    count++;
    i--;
}
return count;
}
int main() {
    char s[] = "Hello World";
    int length = lengthOfLastWord(s);
    printf("Length of last word: %d\n", length);
    return 0;
}
